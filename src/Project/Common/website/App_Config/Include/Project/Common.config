<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <sc.variable name="rootHostName" value="Toast" />
    <settings>
      <setting name="Analytics.CookieDomain" value="$(rootHostName)" />
      <setting name="Login.BackgroundImageUrl" value="login/Toast_Wallpaper.jpg" />
      <setting name="Login.DisableLicenseInfo" value="false" />
      <setting name="Preview.DefaultSite">
        <patch:attribute name="value" value="PushTest" />
      </setting>
    </settings>
    <sites>
      <site name="website">
        <patch:delete />
      </site>
    </sites>
    <cacheSizes>
      <sites>
        <website>
          <patch:delete />
        </website>
      </sites>
    </cacheSizes>
  </sitecore>
</configuration>
